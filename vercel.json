{
  "version": 2,
  "buildCommand": "next build",
  "outputDirectory": ".next",
  "builds": [
    {
      "src": "src/app/**/*.{js,ts,jsx,tsx}",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/src/app/$1"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "@next_public_api_url",
    "NEXT_PUBLIC_CREFAZ_LOGIN": "@next_public_crefaz_login",
    "NEXT_PUBLIC_CREFAZ_SENHA": "@next_public_crefaz_senha",
    "NEXT_PUBLIC_CREFAZ_API_KEY": "@next_public_crefaz_api_key",
    "MONGODB_URI": "@mongodb_uri",
    "NEXT_PUBLIC_URL": "@nex_public_url",
    "SECRET_TOKEN": "@secret_token"
  }
}
